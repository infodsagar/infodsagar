function sumFibs(num) {
let finalR = [1,1]; 
let z = 1;
let y = z;
let x = y;
let sum = 0;

for(let i = 0; finalR[i] < num; i++){
x = y;
y = z;
z = x + y;
finalR.push(z);
}

for(let i = 0; finalR[i] < num+1; i++){
if(finalR[i] % 2 != 0){
sum = sum + finalR[i];
}
}

console.log(finalR);
console.log(sum);
return sum;
}


sumFibs(75025);
